{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor-plugin/templates",
                "version": "4.16.2",
                "type": "ckeditor-plugin",
                "dist": {
                    "url": "https://download.ckeditor.com/templates/releases/templates_4.16.2.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "bower-asset/lazysizes": "^5.3",
        "ckeditor-plugin/templates": "^4.16",
        "composer/installers": "^2.0",
        "cweagans/composer-patches": "^1.7",
        "drupal/admin_toolbar": "^3.6",
        "drupal/ajax_comments": "^1.0@beta",
        "drupal/backup_migrate": "^5.1",
        "drupal/better_exposed_filters": "^7.0",
        "drupal/better_social_sharing_buttons": "^4.0",
        "drupal/bootstrap5": "^3.0",
        "drupal/captcha": "^2.0",
        "drupal/ckeditor": "^1.0",
        "drupal/ckeditor5_embedded_content": "^1.0@alpha",
        "drupal/ckeditor5_paste_filter": "^1.1",
        "drupal/comments_order": "^3.0",
        "drupal/config_ignore": "^3.3",
        "drupal/core-composer-scaffold": "^10.4",
        "drupal/core-project-message": "^10.4",
        "drupal/core-recommended": "^10.4",
        "drupal/csv_serialization": "^4.0",
        "drupal/ctools": "^4.1",
        "drupal/date_filter": "^1.0",
        "drupal/default_content": "^2.0@alpha",
        "drupal/devel": "^5.0",
        "drupal/ds": "^3.13",
        "drupal/easy_breadcrumb": "^2.0",
        "drupal/entity_reference_revisions": "^1.12",
        "drupal/feeds": "^3.0@beta",
        "drupal/ga4_google_analytics": "^1.1",
        "drupal/gin": "^3.1",
        "drupal/gin_login": "^1.0",
        "drupal/gin_toolbar": "^1.1",
        "drupal/google_tag": "^1.8",
        "drupal/image_replace": "^2.0",
        "drupal/inline_entity_form": "^3.0@RC",
        "drupal/jquery_ui": "^1.7",
        "drupal/jquery_ui_datepicker": "^2.1",
        "drupal/jquery_ui_slider": "^2.1",
        "drupal/jquery_ui_touch_punch": "^1.1",
        "drupal/layout_builder_styles": "^2.1",
        "drupal/layout_library": "^1.0@beta",
        "drupal/link_attributes": "^2.1",
        "drupal/linkit": "^7.0",
        "drupal/mail_login": "^4.2",
        "drupal/menu_item_extras": "^2.19",
        "drupal/menu_link_attributes": "^1.5",
        "drupal/metatag": "^2.1",
        "drupal/paragraphs": "^1.12",
        "drupal/password_policy": "^4.0",
        "drupal/pathauto": "^1.13",
        "drupal/recaptcha": "^3.4",
        "drupal/redirect": "^1.11",
        "drupal/save_edit": "^2.1",
        "drupal/simple_sitemap": "^4.1",
        "drupal/social_api": "^4.0",
        "drupal/social_auth": "^4.1",
        "drupal/social_auth_facebook": "^4.0",
        "drupal/svg_image": "^3.0",
        "drupal/symfony_mailer_lite": "^1.0",
        "drupal/telephone_validation": "^2.5",
        "drupal/token": "^1.15",
        "drupal/twig_tweak": "^3.2",
        "drupal/users_export": "^3.0",
        "drupal/views_data_export": "^1.6",
        "drupal/webform": "^6.2@beta",
        "drush/drush": "^12",
        "mpdf/mpdf": "^8.0",
        "npm-asset/jquery-ui-touch-punch": "^0.2.3",
        "oomphinc/composer-installers-extender": "^2.0",
        "symfony/yaml": "^6.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "alpha",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "oomphinc/composer-installers-extender": true
        }
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-types": [
            "bower-asset",
            "npm-asset",
            "ckeditor-plugin"
        ],
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "web/libraries/ckeditor/plugins/{$name}": [
                "type:ckeditor-plugin"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        },
        "patches": {
        }
    }
}
