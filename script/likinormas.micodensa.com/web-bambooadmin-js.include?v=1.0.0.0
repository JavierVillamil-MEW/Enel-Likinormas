function loadTimeRange() {
    var initTime = $('#initTime').val();
    var initHour = initTime.substring(0, initTime.indexOf(':'));
    var initMinute = initTime.substring(initTime.indexOf(':') + 1, initTime.indexOf(' '));
    var initP = initTime.substring(initTime.indexOf(' ') + 1).replace('.', '').replace('.','');

    $('#TimeStart').val(initTime);
    $('#h_TimeStart option[selected=true]').removeAttr('selected');
    $('#h_TimeStart option[value=' + initHour + ']').attr('selected', true);
    $('#m_TimeStart option[selected=true]').removeAttr('selected');
    $('#m_TimeStart option[value=' + initMinute + ']').attr('selected', true);
    $('#p_TimeStart option[selected=true]').removeAttr('selected');
    $('#p_TimeStart option[value=' + initP + ']').attr('selected', true);

    var endTime = $('#endTime').val();
    var endHour = endTime.substring(0, endTime.indexOf(':'));
    var endMinute = endTime.substring(endTime.indexOf(':') + 1, endTime.indexOf(' '));
    var endP = endTime.substring(endTime.indexOf(' ') + 1).replace('.', '').replace('.', '');
    //alert('endHour:' + endHour + "*\nendMinute:" + endMinute + '*\nendP:' + endP + '*');

    $('#TimeEnd').val(endTime);
    $('#h_TimeEnd option[selected=true]').removeAttr('selected');
    $('#h_TimeEnd option[value=' + endHour + ']').attr('selected', true);
    $('#m_TimeEnd option[selected=true]').removeAttr('selected');
    $('#m_TimeEnd option[value=' + endMinute + ']').attr('selected', true);
    $('#p_TimeEnd option[selected=true]').removeAttr('selected');
    $('#p_TimeEnd option[value=' + endP + ']').attr('selected', true);
}

