function _month_get_num_days(month, year) {
    var temp_date;
    if (month == -1) {
        return 31;
    }
    temp_date = new Date(year ? year : 1912, month, 0);
    return temp_date.getDate();
}

function initUploadify(control, uploadUrl, fileExtensions, fileDescription, authenticationToken,
            errorFunction, completeFunction, sizeLimit, multi, auto) {
    //alert('initUploadify');

    var options = {};

    options.script = uploadUrl;
    options.uploader = '/../Includes/Flash/uploadify.swf'
    options.cancelImg = '/../Includes/Style/Images/cancel.png';
    options.scriptData = { AuthenticationToken: authenticationToken };
    options.fileExt = fileExtensions;
    options.fileDesc = fileDescription;

    if (errorFunction != null) {
        options.onError = errorFunction;
    }

    if (completeFunction != null) {
        options.onComplete = completeFunction;
    }

    options.sizeLimit = sizeLimit;
    if (multi == 'True') {
        options.multi = true;
    } else {
        options.multi = false;
    }

    if (auto == 'True') {
        options.auto = true;
    } else {
        options.auto = false;
    }

    /*alert('initUploadify sizeLimit:' + sizeLimit + ' multi:' + multi +
     ' auto:' + auto+ ' op.multi:' + options.multi + ' op.auto:' + options.auto);
   */
    control.uploadify(options);
}

