$(document).ready(function() {
    $('.autoToggle').click(function() {
        var obj = "#" + $(this).attr("rel");
        $(obj).slideToggle("slow");
        return false;
    });

    $(".replaceinput").addClass('replaceinput-inactive');

    $(".replaceinput").bind('focus', function(e) {

        var val = $(this).attr('replacetext');
        if (!val) {
            $(this).attr('replacetext', $(this).val());
        }
        $(this).addClass('replaceinput-active');
        if ($(this).val() == $(this).attr('replacetext')) {
            $(this).val('');
        }
    });

    $('.replaceinput').bind('blur', function(e) {
        
        if ($(this).val() == '') {
            $(this).val($(this).attr('replacetext'));
            $(this).removeClass('replaceinput-active');
        }
    });

});
$(function() {
    var searchboxdefault;
    $('#searchbox').focus(function() {
        if (!searchboxdefault) {
            searchboxdefault = $(this).val();
        }
        if ($(this).val() == searchboxdefault) {
            $(this).val('');
        }
        $(this)
            .removeClass('searchbox-focus');

    });
    $('#searchbox').blur(function() {
        var current = $(this).val();
        if (!current) {
            $(this).val(searchboxdefault);
            $(this).addClass('searchbox-focus');
        }
    });
});
